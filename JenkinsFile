pipeline
{
   agent any
   stages
   {
   stage('build')
   {
    steps
    {
    echo 'building the application...'
        sh './mvnw clean compile'
    }
   }
   stage('test')
      {
       steps
       {
        echo 'testing the application...'
           sh './mvnw test'
       }
      }
      stage('deploy')
            {
             steps
             {
              echo 'deploying the application...'
                 sh './mvnw test'
             }


            }
    }
}

